Cell["\<\
We now transform the tree structure of an expression in a way that each \
primitive operation (called \[OpenCurlyDoubleQuote]part\
\[CloseCurlyDoubleQuote]) is uniquly marked (which then allows to identify it \
with a physical \[OpenCurlyDoubleQuote]device\[CloseCurlyDoubleQuote]). For \
simplicity, the \[OpenCurlyDoubleQuote]devices\[CloseCurlyDoubleQuote] are \
just enumerated (they would be most likely given names when written out in a \
HDL by humans, or some \[OpenCurlyDoubleQuote]temporary variable\
\[CloseCurlyDoubleQuote] names when being used in a code generation as \
typically done for FORTRAN/C).\
\>", "Text",
 CellChangeTimes->{{3.783890655811698*^9, 3.783890663384857*^9}, {
  3.784153632071897*^9, 
  3.7841538339575167`*^9}},ExpressionUUID->"b22347b9-ed1a-41fb-af21-\
27b9811b5678"],

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"aexpr", " ", "is", " ", "the", " ", "output", " ", "of", " ", 
    RowBox[{"Mathematica2AnalogParadigm", "[", 
     RowBox[{"some", " ", "equation", 
      RowBox[{"(", "s", ")"}]}], "]"}]}], " ", "*)"}], "\[IndentingNewLine]", 
  
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"exprParts", "[", "aexpr_", "]"}], " ", ":=", " ", 
     RowBox[{
      RowBox[{
       RowBox[{"Cases", "[", 
        RowBox[{
         RowBox[{"indent", "@", "aexpr"}], ",", " ", 
         RowBox[{"Blank", "@", "#"}], ",", " ", "Infinity"}], "]"}], "&"}], "/@",
       "ElementaryParts"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"exprDevsWithArgs", "[", "aexpr_", "]"}], ":=", 
    RowBox[{"MapIndexed", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"Apply", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"devName", "[", 
           RowBox[{"Head", "@", "#1"}], "]"}], "[", 
          RowBox[{"Last", "@", "#2"}], "]"}], ",", "#1"}], "]"}], "&"}], ",", 
      " ", 
      RowBox[{"exprParts", "@", "aexpr"}], ",", 
      RowBox[{"{", "2", "}"}]}], "]"}]}], "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"args", " ", "still", " ", "parts"}], ",", " ", 
     RowBox[{"not", " ", "devs"}]}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"exprPart2DevsWArgs", "[", "aexpr_", "]"}], " ", ":=", " ", 
     RowBox[{"Replace", "[", 
      RowBox[{
       RowBox[{"Transpose", "@", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Flatten", "@", 
           RowBox[{"exprParts", "@", "aexpr"}]}], ",", 
          RowBox[{"Flatten", "@", 
           RowBox[{"exprDevsWithArgs", "@", "aexpr"}]}]}], "}"}]}], " ", ",", 
       "\[IndentingNewLine]", 
       RowBox[{"List", "\[Rule]", "Rule"}], ",", 
       RowBox[{"{", "2", "}"}], ",", 
       RowBox[{"Heads", "\[Rule]", "True"}]}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"uniqueAnalogExpr", "[", "aexpr_", "]"}], ":=", 
     RowBox[{"aexpr", "//.", 
      RowBox[{"exprPart2DevsWArgs", "[", "aexpr", "]"}]}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"Use", " ", 
     RowBox[{"subby", "@", 
      RowBox[{"shortDevName", "[", "yourExpression", "]"}]}], " ", "for", " ",
      "nicer", " ", "printing"}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"shortDevSymbols", "=", " ", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"devName", "@", 
