Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"allSubtrees", "[", "aexpr_", "]"}], ":=", 
   RowBox[{"Flatten", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"Cases", "[", 
       RowBox[{
        RowBox[{"indent", "@", 
         RowBox[{"uniqueAnalogExpr", "@", "aexpr"}]}], ",", 
        RowBox[{
         RowBox[{"#", "[", "_", "]"}], "[", "__", "]"}], ",", "Infinity", ",",
         " ", 
        RowBox[{"Heads", "\[Rule]", "True"}]}], "]"}], "&"}], "/@", 
     RowBox[{"devName", "/@", "AnalogParadigmParts"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"edgeList", "[", "n_", "]"}], ":=", 
   RowBox[{"MapIndexed", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"{", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"LeafCount", "@", "#"}], ">", "1"}], ",", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"Head", "@", "#"}], ")"}], "[", "outJack", "]"}], ",", 
          "#"}], "]"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"Head", "@", "n"}], ")"}], "[", 
         RowBox[{"inJack", "[", 
          RowBox[{"First", "@", "#2"}], "]"}], "]"}]}], " ", "}"}], "&"}], 
     ",", 
     RowBox[{"List", "@@", "n"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"wiring", "[", "aexpr_", "]"}], ":=", 
   RowBox[{"Flatten", "[", 
    RowBox[{
     RowBox[{"edgeList", "/@", 
      RowBox[{"allSubtrees", "@", "aexpr"}]}], ",", " ", "1"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"wiringWithoutJacks", "[", "aexpr_", "]"}], ":=", 
   RowBox[{
    RowBox[{"wiring", "@", "aexpr"}], "//.", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"x_", "[", 
